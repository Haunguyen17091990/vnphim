            var pop_flag = 0;
            function hdonline_pop_GetCookie(name){
                var re = new RegExp(name+"=[^;]+", "i");
                if (document.cookie.match(re))
                    return decodeURIComponent(document.cookie.match(re)[0].split("=")[1]);
                return "";
            }

            function hdonline_pop_SetCookie(name, value, days){
                if (typeof days!="undefined"){
                    var x= new Date().getTime() + days;
                    var expireDate = new Date();
                    expireDate.setTime(x);
                    document.cookie = name+"="+decodeURIComponent(value)+"; expires="+expireDate.toUTCString();
                } else document.cookie = name+"="+decodeURIComponent(value);
            }

            function hdonline_pop() {
                var hdonline_cookie_pop = hdonline_pop_GetCookie("hdonline_pop");
                if (hdonline_cookie_pop == "") {
                    if (pop_flag == 0) {
                        pop_flag = 1;
                        var date = new Date();
                        var h = date.getHours();
                        var m = date.getMinutes();
                        if ((h >= 7 && m >= 30) && (h <= 22 && m <= 59)){ //15%
                            var Time_expires = 24*3600*1000;
                        } else { // 100%
                            var Time_expires = 6*3600*1000;
                        }

                        hdonline_pop_SetCookie("hdonline_pop", "true", Time_expires);
